    "def perfect():\n",
    "    n=int(input('enter a number'))\n",
    "    sum=0\n",
    "    for i in range(1,n):\n",
    "        if(n%i==0):\n",
    "            sum=sum+i\n",
    "    if(n==sum):\n",
    "        print(\"The number is a perfect number\")\n",
    "    else:\n",
    "        print(\"The number is not a perfect number\")\n",
    "    pass\n",
    "   \n",
    "perfect()"

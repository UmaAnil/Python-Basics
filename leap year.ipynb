    "def leap():\n",
    "    year=int(input('enter a year'))\n",
    "    if(year%4==0 and year%100!=0 or year%400==0):\n",
    "        print('The year is leap year')\n",
    "    else:\n",
    "        print('year is not leap year')\n",
    "    pass\n",
    "\n",
    "leap()"
